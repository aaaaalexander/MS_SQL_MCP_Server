{
  "sqlmcp": {
    "command": "python",
    "args": [
      "sql_mcp_server.py"
    ],
    "env": {
      "PYTHONUNBUFFERED": "1",
      "DB_SERVER": "your_server_name",
      "DB_NAME": "your_database_name",
      "DB_USERNAME": "your_username",
      "DB_PASSWORD": "your_password",
      "DB_ALLOWED_SCHEMAS": "[\"dbo\"]",
      "DB_DEBUG": "true",
      "DB_LOG_LEVEL": "DEBUG"
    }
  }
}